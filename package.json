{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "preview": "nuxt preview",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore . --ignore-pattern libs/getAsyncData.ts",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "test": "jest",
    "postinstall": "[[ -d '.git' ]] && husky install || true"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix --ignore-pattern libs/getAsyncData.ts",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "yarn prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@element-plus/nuxt": "^1.0.4",
    "@types/geojson": "^7946.0.10",
    "core-js": "^3.25.3",
    "diff": "^5.1.0",
    "element-plus": "^2.2.27",
    "maplibre-gl": "^3.2.2",
    "underscore": "^1.13.6",
    "v-lazy-component": "^3.0.9",
    "vue": "^3.2.47",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@types/diff": "^5.0.2",
    "@types/node": "^18.16.0",
    "@types/underscore": "^1.11.4",
    "babel-jest": "^29.1.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.4.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4"
  }
}
