{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "TYPESCRIPT_CHECK=true nuxt dev",
    "generate": "TYPESCRIPT_CHECK=true nuxt generate",
    "start": "nuxt start",
    "preview": "nuxt preview",
    "lint:js": "eslint '**/*.{js,ts,vue}' --cache",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix",
    "prepare": "husky install",
    "test": "jest",
    "postinstall": "[[ -d '.git' ]] && husky install || true"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@element-plus/nuxt": "^1.0.4",
    "@turf/bbox": "^6.5.0",
    "@turf/boolean-equal": "^6.5.0",
    "@types/diff": "^5.0.9",
    "@types/geojson": "^7946.0.10",
    "dayjs": "^1.11.9",
    "diff": "^5.1.0",
    "element-plus": "^2.2.27",
    "maplibre-gl": "^3.2.2",
    "underscore": "^1.13.6",
    "v-lazy-component": "^3.0.9",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.0",
    "@babel/core": "^7.21.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nuxt/eslint": "^0.3.1",
    "@nuxtjs/i18n": "^8.0.0-rc.2",
    "@types/node": "^18.16.0",
    "@types/underscore": "^1.11.4",
    "babel-jest": "^29.1.2",
    "eslint": "^8.57.0",
    "husky": "^8.0.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.4.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint . --cache --fix",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "yarn prettier --write --ignore-unknown"
  }
}
